<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             BackgroundColor="White"
             x:Class="AppGuiaCursoDS.Views.ComponentesSegundo">
    <ContentPage.Content>
        <StackLayout VerticalOptions="CenterAndExpand" Padding="10">

            <Button Text="Ética e Cidadania Organizacional" Clicked="Btn_Etica"  Style="{ StaticResource Botaopreto }" />
            <Button Text="Desenvolvimento de Sistemas" Clicked="Btn_DS" Style="{ StaticResource Botaopreto }" />
            <Button Text="Banco de Dados II " Clicked="Btn_BDadosII" Style="{ StaticResource Botaopreto }" />
            <Button Text="Sistemas Embarcados" Clicked="Btn_SistEmb" Style="{ StaticResource Botaopreto }" />
            <Button Text="Programação de Aplicativos Mobile I " Clicked="Btn_MobI" Style="{ StaticResource Botaopreto }" />
            <Button Text="Programação Web II" Clicked="Btn_PWebII" Style="{ StaticResource Botaopreto}" />

        </StackLayout>

    </ContentPage.Content>
</ContentPage>